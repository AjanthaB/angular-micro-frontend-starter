    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.0.0",
    "serve": "^14.2.0",
    "typescript": "~5.1.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
