{
  "name": "hmt-web-mono",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:shell",
    "start:shell": "ng serve shell  -o --port 5000",
    "start:dashboard": "ng serve dashboard -o --port 3000",
    "start:all": "concurrently \"npm run start:shell\" \"npm run start:dashboard\" ",
    "build:shell": "ng build shell",
    "build:dashboard": "ng build dashboard ",
    "serve:dist": "concurrently \"serve dist/shell -l 5000 -s\" \"serve dist/mfe1 -l 3000 -s\" ",
    "build": "npm run build:shell && npm run build:dashboard",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^16.0.4",
